/*esilo css Examen Antonio*/
h1{
	text-align:center;
	font-family:'Comic Sans MS';
	font-size:45px;
	color:#003869;
}

.cuerpo {
	width: 1000px;
	background-color:#FFFBD7;
	margin:10px auto 10px auto;
	height: 600px;
}

.cabecera {
	width: 100%;
	height: 150px;
	color:red;
	font-size:62px;
	font-weight:bold;
	background-color:#FFFBEF;
	float: left;
	text-shadow: -5px -5px 5px #aaa;
}

.centro{
	width:100%;
	height:420px;
	padding:20px 0px 20px 20px;
	float: left;
	background-color:#FFFBD7;
}	
.i{
	width:140px;
	height:225px;
	float: left;
	background-color:#52B3C5;
	margin:6px 3px 0px 4px;
	padding: 10px;
	/*para Firefox*/
	-moz-border-radius: 15px 0px 15px 0px;
	/*para Safari y Chrome*/
	-webkit-border-radius: 15px 0px 15px 0px;
	/* para Opera */
	border-radius: 15px 0px 15px 0px;
}
.menui {
		width: 160;
		height: 200px;
		float: left;
		}
	.menud {
		width: 500px;
		height: 35px;
		background-color:#521328;
		text-align:right;
		color:white;}
	.menui {
		list-style-type:none;	/*esto hace que no tenga punto la lista desordenada*/
		margin:0;				/*esto elimina el margen del menu*/
		padding:0;}				/*esto elimina la separacion*/
			.menui li {
				margin:3px;			/*nos va a dar la separacion entre las palabras*/
				padding:4px;		/*hace que se ponga como menu*/
				}		
				.menui li a {
						width:120px;
						padding:4px;
						text-decoration:none;	/*quita el subrayado del a*/
						text-align:center;		/*alinea el texto dentro de los cuadros*/
						font-size:15px;			/*tamaño de letra*/
						color:#FFF;				/*color de la letra*/
						background-color:003869;	/*color de fondo del menu*/
						/*border-left:10px solid #666;	/*tipo de borde del menu_lo añade solo a la izquierda, tamaño, tipo de linea, color*/
						border-radius: 15px 15px 15px 15px}	/*redondea*/
						.menui li a:hover{				/*se utiliza para cuando pasamos sobre el menu con el raton*/
								color:white;
								background-color:#003869;		/*cambia el color del fondo*/
								border-left-color:#FCE6B8;}	/*cambiar el color de border izquierdo solo cuando pasamos sobre el menu*/

.c{
	width:591px;
	height:345px;
	float: left;
	background-color:#CEE2FF;
	padding:30px 30px 30px 30px;
	text-align:justify;
	margin:6px 0px 4px 0px;
}
.d{
	width:165px;
	height:195px;
	float: left;
	padding:5px;
	background-color:#60D1E5;
	margin:6px 4px 0px 3px;
}

.pie {
	width: 100%;
	height: 20px;
	float: left;
	text-align:center;
	color:#CEE2FF;
	background-color:#A8BEFF;
	-moz-border-radius: 1px 10px 10px 10px;
	/*para Safari y Chrome*/
	-webkit-border-radius: 10px 10px 10px 10px;
	/* para Opera */
	border-radius: 10px 10px 10px 10px;
}